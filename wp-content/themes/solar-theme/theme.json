{
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#f8f9fa",
					"name": "Base Secondary",
					"slug": "base-secondary"
				},
				{
					"color": "#212529",
					"name": "Dark",
					"slug": "dark"
				},
				{
					"color": "#343a40",
					"name": "Dark Secondary",
					"slug": "dark-secondary"
				},
				{
					"color": "#009CFF",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#6C757D",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#28A745",
					"name": "Success",
					"slug": "success"
				},
				{
					"color": "#17A2B8",
					"name": "Info",
					"slug": "info"
				},
				{
					"color": "#FFC107",
					"name": "Warning",
					"slug": "warning"
				},
				{
					"color": "#DC3545",
					"name": "Danger",
					"slug": "danger"
				},
				{
					"color": "#F8F9FA",
					"name": "Light",
					"slug": "light"
				}
			]
		},
		"layout": {
			"contentSize": "1140px",
			"wideSize": "1320px"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "2rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "3rem",
					"slug": "large"
				},
				{
					"name": "X-Large",
					"size": "5rem",
					"slug": "x-large"
				}
			],
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "'Montserrat', sans-serif",
					"name": "Montserrat",
					"slug": "montserrat"
				},
				{
					"fontFamily": "'Roboto', sans-serif",
					"name": "Roboto",
					"slug": "roboto"
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.25rem",
						"max": "1.5rem"
					},
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.5rem"
					},
					"name": "X-Large",
					"size": "2.5rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "3rem",
						"max": "4rem"
					},
					"name": "XX-Large",
					"size": "4rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"min": "4rem",
						"max": "6rem"
					},
					"name": "Display 1",
					"size": "6rem",
					"slug": "display-1"
				},
				{
					"fluid": {
						"min": "3.5rem",
						"max": "5.5rem"
					},
					"name": "Display 2",
					"size": "5.5rem",
					"slug": "display-2"
				},
				{
					"fluid": {
						"min": "3rem",
						"max": "4.5rem"
					},
					"name": "Display 3",
					"size": "4.5rem",
					"slug": "display-3"
				},
				{
					"fluid": {
						"min": "2.5rem",
						"max": "3.5rem"
					},
					"name": "Display 4",
					"size": "3.5rem",
					"slug": "display-4"
				}
			]
		},
		"useRootPaddingAwareAlignments": true,
		"custom": {
			"spacing": {
				"gutter": "1.5rem"
			},
			"borderRadius": {
				"small": "0.25rem",
				"medium": "0.5rem",
				"large": "1rem"
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--dark)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--montserrat)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		},
		"spacing": {
			"padding": {
				"top": "0",
				"right": "var(--wp--custom--spacing--gutter)",
				"bottom": "0",
				"left": "var(--wp--custom--spacing--gutter)"
			},
			"blockGap": "1.5rem"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700"
				},
				"spacing": {
					"padding": {
						"top": "0.75rem",
						"right": "1.25rem",
						"bottom": "0.75rem",
						"left": "1.25rem"
					}
				},
				"border": {
					"radius": "var(--wp--custom--border-radius--small)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--dark)",
						"text": "var(--wp--preset--color--base)"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--dark)"
					}
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontWeight": "700",
					"lineHeight": "1.2"
				},
				"color": {
					"text": "var(--wp--preset--color--dark)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--display-2)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--display-3)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"blocks": {
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500"
				}
			},
			"core/button": {
				"variations": {
					"outline": {
						"color": {
							"background": "transparent",
							"text": "var(--wp--preset--color--primary)"
						},
						"border": {
							"color": "var(--wp--preset--color--primary)",
							"width": "2px",
							"style": "solid"
						},
						":hover": {
							"color": {
								"background": "var(--wp--preset--color--primary)",
								"text": "var(--wp--preset--color--base)"
							}
						}
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontSize": "var(--wp--preset--font-size--display-4)",
					"fontWeight": "700",
					"textTransform": "uppercase"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--base)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--base)"
							}
						}
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"customTemplates": [
		{
			"name": "page-full-width",
			"postTypes": ["page"],
			"title": "Full Width Page"
		},
		{
			"name": "blank",
			"postTypes": ["page", "post"],
			"title": "Blank"
		}
	]
}
